<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"

    xmlns:reskeys="clr-namespace:MRULib.Themes"
    xmlns:sys="clr-namespace:System;assembly=mscorlib"
    >
    <!-- Pin Control -->
    <SolidColorBrush x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type reskeys:ResourceKeys},
                                ResourceId=Pin_Foreground}" Color="#C0FFFFFF" />

    <SolidColorBrush x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type reskeys:ResourceKeys},
                                ResourceId=Pin_ForegroundBorder}" Color="#FFFFFFFF" />

    <SolidColorBrush x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type reskeys:ResourceKeys},
                                ResourceId=Hyperlink_Foreground}" Color="#FFFEFEFE" />

    <sys:Double x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type reskeys:ResourceKeys},
                                ResourceId=Pin_NoCheckMarkOpacitiy}">0.75</sys:Double>
    
</ResourceDictionary>
